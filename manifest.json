{
    "manifest_version": 2,
    "name": "TreeStyleChrome",
    "description": "Tree style tabs",
    "version": "1.0",
    "author": "unisay@gmail.com",
    "permissions": [
        "tabs", "<all_urls>"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "match_about_blank": true,
            "run_at": "document_start",
            "css": [ "css/content.css" ],
            "js": [ "react/react-0.10.0.js", "js/content.js" ]
        }
    ],
    "web_accessible_resources": [
        "css/iframe.css", "img/chrome-32.png", "img/loading.gif", "font/oswald.woff"
    ],
    "browser_action": {
        "default_icon": "img/icon.png"
    }
}