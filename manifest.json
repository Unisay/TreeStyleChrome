{
    "manifest_version": 2,
    "name": "TreeStyleChrome",
    "description": "Tree style tabs",
    "version": "1.0",
    "author": "unisay@gmail.com",
    "permissions": [
        "tabs", "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "match_about_blank": true,
            "run_at": "document_start",
            "css": [ "content.css" ],
            "js": [ "content.js" ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}